    "bootstrap": "3.3.7",
    "cookie-parser": "1.4.3",
    "css-loader": "^0.28.0",
    "debug": "2.6.0",
    "es6-shim": "^0.35.3",
    "express": "4.14.1",
    "express-react-views": "0.10.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "glob": "^7.1.2",
    "highlight.js": "^9.12.0",
    "inquirer": "^3.1.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "mkdirp": "^0.5.1",
    "mkpath": "^1.0.0",
    "morgan": "1.7.0",
    "object-assign": "^4.1.1",
    "postcss-loader": "^1.3.3",
    "raw-loader": "^0.5.1",
    "react": "15.4.2",
    "react-bootstrap": "0.30.7",
    "react-docgen-typescript": "1.0.2",
    "react-dom": "15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
